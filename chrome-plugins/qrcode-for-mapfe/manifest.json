{
    "manifest_version":2,
    "name":"极简二维码",
    "description":"极简二维码",
    "version":"1.0",
    "icons": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "background": { "scripts": ["background.js"] },
    "permissions": ["tabs"],
    "browser_action":{
        "default_icon":"images/icon.png",
        "default_title":"极简二维码",
        "default_popup":"popup.html"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+P",
                "mac": "MacCtrl+Shift+P"
            },
            "description": "Opens popup.html"
        }
    }
     
}