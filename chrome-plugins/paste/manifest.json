{
    "manifest_version":2,
    "name":"auto paste",
    "description":"自动粘贴",
    "version":"1.0",
    "icons": {
        "16": "images/paste.png",
        "48": "images/paste.png",
        "128": "images/paste.png"
    },
    "permissions": ["activeTab", "clipboardRead"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "permissions": [
                "activeTab", "clipboardRead"
            ],
            "all_frames": true,
            "run_at": "document_idle"
        }
    ]
}