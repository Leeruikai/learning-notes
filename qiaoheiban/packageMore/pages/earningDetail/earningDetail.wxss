/* packageMore/pages/earningDetail/earningDetail.wxss */
.earning-page-container {
  height: 100%;
  display: flex;
  flex-direction: column;
}
.earning-page-header {
  background-color: #fff;
  height: 260rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.page-title {
  height: 120rpx;
  line-height: 120rpx;
  font-size: 36rpx;
  text-align: center
}
.page-keypoints {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}
.page-keypoint {
  flex: 1;
  font-size: 36rpx;
  text-align: center;
}
.page-keypoint text {
  font-size: 24rpx;
}

.earning-page-content {
  flex: 1;
  padding: 50rpx 30rpx;
}